@model IEnumerable<ESVersions.Models.Software>


<table class="table table-bordered">
    <thead>
        <tr>
            <th>Title</th>
            <th>Version</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.Encode(item.Name)
                </td>
                <td>
                    @Html.Encode(item.Version)
                </td>
            </tr>        
        }
    </tbody>
</table>